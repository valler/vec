{
  "exclude": [
    "lib/deno",
    "lib/scripts",
    "lib/cov",
    "sw.ts"
  ],
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "nodenext",
    "lib": [
      "ESNext",
      "DOM",
      "DOM.Iterable"
    ],
    "sourceMap": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "noImplicitAny": true
  }
}